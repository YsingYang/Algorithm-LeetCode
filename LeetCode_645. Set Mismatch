class Solution {
public:
    vector<int> findErrorNums(vector<int>& nums) {
        vector<int> res(2, 0);
        int size = nums.size();
        vector<int> count(size + 1, 0);
        for(int i = 0; i < size; ++i){
            ++count[nums[i]];
        }
        for(int i = 1; i <= size; ++i){
            if(count[i] == 2)
                res[0] = i;
            else if(count[i] == 0){
                res[1] = i;
            }
        }
        return res;
        
    }
};
