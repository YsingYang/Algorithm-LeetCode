class Solution {
public:
    vector<vector<int>> flipAndInvertImage(vector<vector<int>>& A) {
        int cs = A[0].size(); //A 不为空
        for(vector<int>& vec : A) {
            vector<int> temp = vec;
            for(int i = 0; i <vec.size(); ++i) {
                vec[i] =  1 - temp[cs-i-1];
            }            
        }
        return A;
    }
};
